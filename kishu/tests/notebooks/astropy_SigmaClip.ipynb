   "source": [
    "from astropy.stats import SigmaClip\n",
    "import numpy as np\n",
    "clip = SigmaClip(sigma=2, maxiters=5)\n",
    "data = np.random.normal(0, 1, 100)\n",
    "data[::10] += 10\n",
    "clipped = clip(data)\n",
    "print(clipped)"
   ]
