   "source": [
    "import numpy as np\n",
    "from astropy.modeling.functional_models import Linear1D\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "56cca2c4",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Linear1D(slope=2.5, intercept=1.0)\n",
    "x = np.linspace(-10, 10, 100)\n",
    "y = model(x)\n",
    "plt.plot(x, y)\n",
    "plt.title('Linear1D Model')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  }
