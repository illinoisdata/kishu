    "import numpy as np\n",
    "w = WCS(naxis=2)\n",
    "w.wcs.crpix = [0, 0]\n",
    "w.wcs.cdelt = np.array([-0.066667, 0.066667])\n",
    "w.wcs.crval = [0, -90]\n",
    "w.wcs.ctype = [\"RA---AIR\", \"DEC--AIR\"]\n",
    "print(w)"
