   "source": [
    "import statsmodels.api as sm\n",
    "import numpy as np\n",
    "X = sm.add_constant(np.arange(10))\n",
    "y = X[:, 1] + np.random.normal(size=10)\n",
    "model = sm.OLS(y, X).fit()\n",
    "print(model.summary())"
   ]
  }
